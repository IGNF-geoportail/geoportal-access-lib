{
  "name": "geoportal-access-lib",
  "version": "1.2.0",
  "description": "French Geoportal resources access library",
  "main": "dist/GpServices.js, dist/GpServices-src.js, dist/GpServices-src.js.map",
  "scripts": {
    "setup": "npm install",
    "build": "npm run setup && webpack",
    "build:prod": "npm run setup && webpack --env.production",
    "test": "npm run setup && mocha-webpack --webpack-config webpack.test.js --glob \"test_*.js\" test/spec/",
    "test:serve": "webpack-dev-server --hot --config webpack.test.serve.js",
    "sample:serve": "webpack-dev-server --contentBase samples --port 9001 --open",
    "doc:serve": "webpack-dev-server --contentBase jsdoc --port 9001 --open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IGNF/geoportal-access-lib.git"
  },
  "keywords": [
    "geoportail",
    "webservice",
    "javascript",
    "es6"
  ],
  "author": "IGNF",
  "license": "CECILL-B",
  "dependencies": {
    "es6-promise": "^4.2.4",
    "loglevel": "^1.6.1",
    "request": "^2.79.0",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.18",
    "html-webpack-plugin": "^2.30.1",
    "jsdoc-webpack-plugin": "0.0.1",
    "jshint": "^2.9.5",
    "jshint-loader": "^0.8.4",
    "mocha": "^4.0.0",
    "mocha-loader": "^1.1.3",
    "mocha-webpack": "^1.0.1",
    "path": "^0.12.7",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sinon": "^4.3.0",
    "sinon-es6": "0.0.3",
    "string-template": "^1.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "optionalDependencies": {
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-hb": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^5.0.0",
    "handlebars": "^4.0.11",
    "handlebars-helpers": "^0.10.0",
    "handlebars-layouts": "^3.1.4",
    "run-sequence": "^2.2.1"
  }
}
